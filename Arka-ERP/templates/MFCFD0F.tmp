<script>
function getvalue()
{					var quantity = document.getElementById('quantity').value;
					var unit1 = document.getElementById('unit_basic').value;
					var cost = (quantity * unit1);
					var tax = document.getElementById('tax_per').value;
					if(tax != "")
					{
							var tax_value = (cost * tax)/100;
							document.getElementById('sub_total').value=cost;
							document.getElementById('vat_value').value=tax_value;
							var total = (+tax_value + +cost);
							document.getElementById('total').value=total;
					}
					
}
</script>

